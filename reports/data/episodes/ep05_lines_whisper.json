      "mergeReasons": [
        "no_terminal_punctuation",
        "small_gap"
      ]
    },
    {
      "lineId": "ep05-line-056",
      "startMs": 502800,
      "endMs": 505560,
      "text": "ねぇ、あなたたちはどこに向かってるの?",
      "rawEntryIds": [
        "whisper-103"
      ],
      "mergeReasons": []
    },
    {
      "lineId": "ep05-line-057",
      "startMs": 506040,
      "endMs": 506880,
      "text": "地球だよ",
      "rawEntryIds": [
        "whisper-104"
      ],
      "mergeReasons": []
    },
    {
      "lineId": "ep05-line-058",
      "startMs": 507640,
      "endMs": 508880,
      "text": "そっか",
      "rawEntryIds": [
        "whisper-105"
      ],
      "mergeReasons": []
    },
    {
      "lineId": "ep05-line-059",
      "startMs": 510600,
      "endMs": 513720,
      "text": "あのさ、そこからエンケラデスは見えるかな?",
      "rawEntryIds": [
        "whisper-106"
      ],
      "mergeReasons": []
    },
    {
      "lineId": "ep05-line-060",
      "startMs": 514400,
      "endMs": 516880,
      "text": "見えてるよ、ちょうど近くを通ってる",
      "rawEntryIds": [
        "whisper-107"
      ],
      "mergeReasons": []
    },
    {
      "lineId": "ep05-line-061",
      "startMs": 518160,
      "endMs": 520880,
      "text": "そこからどう見える、エンケラデスは?",
      "rawEntryIds": [
